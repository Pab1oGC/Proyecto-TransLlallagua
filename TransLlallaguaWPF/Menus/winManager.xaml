                </StackPanel>
            </Grid>
        </StackPanel>

        <Image x:Name="busImage" Source="/Assets/bus2.png" Width="90" Height="90" Margin="197,140,513,370" ClipToBounds="True" MouseLeftButtonDown="busImage_MouseLeftButtonDown" >
            <Image.Triggers>
                <EventTrigger RoutedEvent="MouseEnter">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="busImage"
                                  Storyboard.TargetProperty="Opacity"
                                  To="0.7" Duration="0:0:0.3"/>
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
                <EventTrigger RoutedEvent="MouseLeave">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="busImage"
                                  Storyboard.TargetProperty="Opacity"
                                  To="1" Duration="0:0:0.3"/>
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Image.Triggers>
            <Image.OpacityMask>
                <ImageBrush ImageSource="/Assets/bus2.png"/>
            </Image.OpacityMask>
        </Image>
        <Image x:Name="keyImage" Source="/Assets/keyO.png" Width="90" Height="90" Margin="10,495,700,15" ClipToBounds="True" MouseLeftButtonDown="keyImage_MouseLeftButtonDown">
